{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "8000"
      },
      "alwaysAllow": [
        "get-library-docs",
        "resolve-library-id"
      ]
    }
  }
}